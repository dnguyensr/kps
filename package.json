{
  "name": "sundry",
  "version": "1.0.0",
  "description": "Sundry is a static travel experience site focused around cities using the collective experience of the studio sundry community. Each city will include information and guides regarding travel planning prior to arrival, cultural sites to visit, places to eat, and the local music scene.",
  "main": "index.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnguyensr/sundry.git"
  },
  "author": "dnguyensr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dnguyensr/sundry/issues"
  },
  "homepage": "https://github.com/dnguyensr/sundry#readme",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",

    "systemjs": "0.19.27",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23",
    "angular2-in-memory-web-api": "0.0.20",
    "express": "^4.14.0",
    "body-parser": "^1.15.2",
    "morgan": "^1.7.0",
    "mongoose": "^4.6.0",
    "font-awesome": "^4.6.3",
    "bootstrap": "^3.3.7"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  }
}
